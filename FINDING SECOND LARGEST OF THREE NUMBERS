#include <stdio.h>
void  main()
{
    int n1,n2,n3;
    printf("enter three numbers:");
    scanf("%d%d%d",&n1,&n2,&n3);
    if(n1>n2&&n1>n2)
    {
        if(n2>n3)
        printf("second largest is %d",n2);
        else
        printf("second largest is %d",n3);
    }
if(n2>n1&&n2>n3)
    {
        if(n1>n3)
        printf("second largest is %d",n1);
        else
        printf("second largest is %d",n3);
    }
if(n3>n2&&n3>n1)
    {
        if(n2>n1)
        printf("second largest is %d",n2);
        else
        printf("second largest is %d",n1);
    }
}
