import java.util.*;
class Main 
{
    public static void main(String []args)
    {
        Scanner sc =new Scanner(System.in);
        int num1=sc.nextInt();
        int num2=sc.nextInt();
        int hcf=0,temp=num1>num2?num2:num1;
        for(int i=1;i<=temp;i++)
        {
            if(num1%i==0&&num2%i==0)
            hcf=i;
        }
        System.out.print(hcf);
    }
}
