#include <stdio.h>
#include<string.h>
int main()
{
    char a[10];
    int i,j,c;
    printf(" enter the string:");
    scanf("%s",a);
    c=strlen(a)-1;
    for(i=0;j=c;i<=j;i++;j--)
    {
        if(a[i]!=a[j])
        {
            printf("Not palindrome ");
            return 0;
        }
    }
    printf("It's palindrome");
}
